<?xml version="1.0" encoding="UTF-8"?>
<!-- Déclaration de la racine du document XML avec l'espace de noms 'cv24' et la spécification de l'emplacement du schéma XSD pour la validation. -->
<cv24:cv24 xmlns:cv24="http://univ.fr/cv24" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://univ.fr/cv24 cv24.tp1.xsd">

  <!-- Section identité contenant les informations personnelles du candidat. -->
  <cv24:identite>
    <cv24:genre>Mme</cv24:genre> <!-- Genre du candidat -->
    <cv24:nom>RACHEL</cv24:nom> <!-- Nom de famille -->
    <cv24:prenom>Rachel</cv24:prenom> <!-- Prénom -->
    <cv24:tel>+33 1 23 45 67 89</cv24:tel> <!-- Numéro de téléphone -->
  </cv24:identite>
  
  <!-- Objectif professionnel du candidat. -->
  <cv24:objectif statut="stage">Stage Chez EDF</cv24:objectif> <!-- Statut de l'objectif professionnel -->
  
  <!-- Expériences professionnelles. -->
  <cv24:prof>
    <!-- Détail d'une expérience professionnelle spécifique. -->
    <cv24:detail>
      <cv24:datedeb>2024-05-20</cv24:datedeb> <!-- Date de début -->
      <cv24:datefin>2024-09-20</cv24:datefin> <!-- Date de fin -->
      <cv24:titre>Developpeur web</cv24:titre> <!-- Titre du poste -->
    </cv24:detail>
    
    <!-- Exemple d'une autre expérience professionnelle, avec un titre à corriger. -->
    <cv24:detail>
      <cv24:datedeb>2001-01-01</cv24:datedeb> <!-- Date de début -->
      <cv24:titre>cv24:titre</cv24:titre> <!-- Titre (devrait être mis à jour avec un titre valide) -->
    </cv24:detail>
  </cv24:prof>
  
  <!-- Compétences académiques et certifications. -->
  <cv24:competences>
    <!-- Détail des diplômes obtenus. -->
    <cv24:diplome niveau="3">
      <cv24:date>2001-01-01</cv24:date> <!-- Date d'obtention -->
      <cv24:institut>Universite d'oran</cv24:institut> <!-- Nom de l'institut -->
    </cv24:diplome>
    <cv24:diplome niveau="5">
      <cv24:date>2025-06-01</cv24:date> <!-- Date d'obtention prévue -->
      <cv24:institut>Universite de Rouen</cv24:institut> <!-- Nom de l'institut -->
    </cv24:diplome>
    
    <!-- Exemple d'un diplôme sans institut spécifié. -->
    <cv24:diplome niveau="8">
      <cv24:date>2001-01-01</cv24:date> <!-- Date d'obtention -->
    </cv24:diplome>
    
    <!-- Certifications obtenues. -->
    <cv24:certif>
      <cv24:datedeb>2001-01-01</cv24:datedeb> <!-- Date d'obtention -->
      <cv24:titre>cv24:titre</cv24:titre> <!-- Titre (devrait être mis à jour avec un titre valide) -->

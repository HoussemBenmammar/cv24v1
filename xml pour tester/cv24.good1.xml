<?xml version="1.0" encoding="UTF-8"?>
<!-- Déclaration de la racine du document XML avec l'espace de noms 'cv24' et la spécification de l'emplacement du schéma XSD pour la validation. -->
<cv24:cv24 xmlns:cv24="http://univ.fr/cv24" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://univ.fr/cv24 cv24.tp1.xsd">
  
  <!-- Section d'identité du candidat, incluant le genre, nom, prénom et adresse e-mail. -->
  <cv24:identite>
    <cv24:genre>Mme</cv24:genre> <!-- Genre du candidat -->
    <cv24:nom>LEMAIRE</cv24:nom> <!-- Nom de famille du candidat -->
    <cv24:prenom>Emma</cv24:prenom> <!-- Prénom du candidat -->
    <cv24:mel>emma.lemaire@example.com</cv24:mel> <!-- Adresse e-mail du candidat -->
  </cv24:identite>

  <!-- Objectif professionnel du candidat avec indication du statut visé. -->
  <cv24:objectif statut="emploi">Ingénieure logiciel</cv24:objectif>

  <!-- Expériences professionnelles du candidat. -->
  <cv24:prof>
    <!-- Détail d'une expérience professionnelle -->
    <cv24:detail>
      <cv24:datedeb>2020-09-01</cv24:datedeb> <!-- Date de début -->
      <cv24:titre>Ingénieure logiciel chez TechnoWeb</cv24:titre> <!-- Titre et entreprise -->
    </cv24:detail>
    <!-- Une autre expérience professionnelle -->
    <cv24:detail>
      <cv24:datedeb>2018-06-01</cv24:datedeb> <!-- Date de début -->
      <cv24:titre>Développeuse Full-stack à InnovTech</cv24:titre> <!-- Titre et entreprise -->
    </cv24:detail>
  </cv24:prof>

  <!-- Section des compétences, incluant les diplômes. -->
  <cv24:competences>
    <!-- Détail d'un diplôme obtenu par le candidat -->
    <cv24:diplome niveau="5">
      <cv24:date>2018-05-01</cv24:date> <!-- Date d'obtention -->
      <cv24:institut>ecole superieur d'informatique</cv24:institut> <!-- Date d'obtention -->
    </cv24:diplome>
  </cv24:competences>
</cv24:cv24>
